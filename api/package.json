  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mollie/api-client": "^3.6.0",
    "@nestjs/cli": "^8.2.4",
    "@nestjs/common": "^8.4.3",
    "@nestjs/core": "^8.4.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/schematics": "^8.0.9",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "auth0": "^2.40.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "express-openid-connect": "^2.7.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "jwt-claims": "^1.0.1",
    "jwt-decode": "^3.1.2",
    "mysql2": "^2.3.3",
    "nestjs-form-data": "^1.6.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sequelize": "^6.18.0",
    "sequelize-typescript": "^2.1.3",
    "stream": "^0.0.2",
    "stripe": "^8.219.0",
    "swagger-ui-express": "^4.3.0",
    "tedious": "^14.4.0",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.4",
    "@types/auth0": "^2.34.17",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "~1.4.5",
    "@types/node": "^17.0.23",
    "@types/passport-jwt": "^3.0.6",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-skipped-reporter": "^0.0.5",
    "mockingbird": "^2.1.2",
    "prettier": "^2.6.2",
    "sequelize-mock": "^0.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
